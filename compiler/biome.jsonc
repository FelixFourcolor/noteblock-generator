{
	"$schema": "https://biomejs.dev/schemas/2.3.7/schema.json",
	"linter": {
		"includes": ["*.ts"],
		"rules": {
			"style": {
				"noParameterAssign": "off",
				"noNonNullAssertion": "off"
			},
			"suspicious": {
				"noExplicitAny": "off"
			},
			"correctness": {
				"useParseIntRadix": "off"
			}
		}
	}
}
