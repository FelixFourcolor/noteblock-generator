{
    "noteName": {
        "title": "Note",
        "description": "Format: [pitch] [duration]",
        "type": "string",
        "pattern": "^[a-gA-G](bb|b|s|ss)?([1-7]?|(_*|\\^*))((\\s+[+-]?|\\s*[+-])(([1-9]\\d*b?)?\\.|[1-9]\\d*b?\\.?))*$"
    },
    "rest": {
        "title": "Rest",
        "description": "Format: R [duration]",
        "type": "string",
        "pattern": "^[rR]((\\s+[+-]?|\\s*[+-])(([1-9]\\d*b?)?\\.|[1-9]\\d*b?\\.?))*$"
    },
    "compoundNote": {
        "title": "Compound note",
        "description": "Format: <noteName>, <noteName>, etc.",
        "type": "string",
        "pattern": "^([a-gA-G](bb|b|s|ss)?([1-7]?|(_*|\\^*))((\\s+[+-]?|\\s*[+-])(([1-9]\\d*b?)?\\.|[1-9]\\d*b?\\.?))*(\\s*,\\s*|$)){2,}$"
    },
    "barDelimiter": {
        "title": "Bar delimiter",
        "description": "| to assert bar line, another | to rest for the entire bar, optionally followed by a bar number, optionally followed by ! to force assertion.",
        "type": "string",
        "pattern": "^\\|{1,2}(\\s*\\d+)?\\!?$"
    },
    "time": {
        "oneOf": [
            { "$ref": "globals.schema.json#/time" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "delay": {
        "oneOf": [
            { "$ref": "globals.schema.json#/delay" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "beat": {
        "oneOf": [
            { "$ref": "globals.schema.json#/beat" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "trillStyle": {
        "oneOf": [
            { "$ref": "globals.schema.json#/trillStyle" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "singleDivisionPosition": {
        "oneOf": [
            { "$ref": "voices.schema.json#/singleDivisionPosition" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "doubleDivisionPosition": {
        "oneOf": [
            { "$ref": "voices.schema.json#/doubleDivisionPosition" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "instrument": {
        "oneOf": [
            { "$ref": "voices.schema.json#/instrument" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "dynamic": {
        "oneOf": [
            { "$ref": "voices.schema.json#/dynamic" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "transpose": {
        "oneOf": [
            { "$ref": "voices.schema.json#/transpose" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "sustain": {
        "oneOf": [
            { "$ref": "voices.schema.json#/sustain" },
            { "$ref": "#/$def/reset" }
        ]
    },
    "$def": {
        "reset": {
            "description": "Set value to environment's default.",
            "const": "$reset"
        }
    }
}